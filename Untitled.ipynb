   "source": [
    "# Write a Python function to multiply all the numbers in a list."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 31,
   "id": "96185cbe",
   "metadata": {},
   "outputs": [],
   "source": [
    "num = [1, 2, 3, -4]"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 40,
   "id": "56b2b3f6",
   "metadata": {},
   "outputs": [],
   "source": [
    "def multiply(num):\n",
    "    result = 1\n",
    "    for num in num:\n",
    "        result *= num\n",
    "    return result"
   ]
  },
